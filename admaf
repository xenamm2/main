local COORDS_SPOT2 = Vector3.new(-287.11, 39.78, -1627.26)

local autoFarmEnabled = false
local autoTeleportEnabled = false
local teleportThread = nil
local farmThread = nil

teleportBtn.MouseButton1Click:Connect(function()
    TweenService:Create(teleportBtn, TweenInfo.new(0.1), {
        BackgroundColor3 = Color3.fromRGB(60, 140, 220),
        Text = "TELEPORTING..."
    }):Play()
    
    print("Starting gingerbread teleport...")
    
    local character = player.Character
    if not character then
        character = player.CharacterAdded:Wait()
    end
    
    local humanoidRootPart = character:WaitForChild("HumanoidRootPart")
    
    local function findGingerbreadModels()
        local gingerbreadModels = {}
        
        for _, model in ipairs(workspace:GetDescendants()) do
            if model:IsA("Model") and (model.Name == "GingerbreadMan" or model.Name == "GingerbreadRig") then
                table.insert(gingerbreadModels, model)
            end
        end
        
        return gingerbreadModels
    end
    
    local gingerbreadList = findGingerbreadModels()
    print("Found " .. #gingerbreadList .. " gingerbread models")
    
    for _, gingerbread in ipairs(gingerbreadList) do
        if gingerbread and (gingerbread:FindFirstChild("HumanoidRootPart") or gingerbread.PrimaryPart) then
            local targetPart = gingerbread.PrimaryPart or gingerbread:FindFirstChild("HumanoidRootPart")
            if targetPart then
                humanoidRootPart.CFrame = targetPart.CFrame + Vector3.new(0, 5, 0)
                print("Teleported to: " .. gingerbread.Name)
                wait(0.6)
            end
        end
    end
    
    TweenService:Create(teleportBtn, TweenInfo.new(0.2), {
        BackgroundColor3 = Color3.fromRGB(80, 160, 255),
        Text = "DONE!"
    }):Play()
    
    wait(0.5)
    
    TweenService:Create(teleportBtn, TweenInfo.new(0.2), {
        BackgroundColor3 = Color3.fromRGB(80, 160, 255),
        Text = "TELEPORT"
    }):Play()
end)

local function toggleAutoFarm()
    autoFarmEnabled = not autoFarmEnabled
    
    if autoFarmEnabled then
        TweenService:Create(toggleFrame2, TweenInfo.new(0.3), {
            BackgroundColor3 = Color3.fromRGB(80, 220, 100)
        }):Play()
        toggleText2.Text = "ON"
        
        print("Auto Farm ENABLED")
        
        farmThread = task.spawn(function()
            while autoFarmEnabled do
                local character = player.Character
                if not character then
                    character = player.CharacterAdded:Wait()
                end
                
                local humanoidRootPart = character:WaitForChild("HumanoidRootPart")
                
                local function findGingerbreadModels()
                    local gingerbreadModels = {}
                    
                    for _, model in ipairs(workspace:GetDescendants()) do
                        if model:IsA("Model") and (model.Name == "GingerbreadMan" or model.Name == "GingerbreadRig") then
                            table.insert(gingerbreadModels, model)
                        end
                    end
                    
                    return gingerbreadModels
                end
                
                local gingerbreadList = findGingerbreadModels()
                
                for _, gingerbread in ipairs(gingerbreadList) do
                    if not autoFarmEnabled then break end
                    
                    if gingerbread and (gingerbread:FindFirstChild("HumanoidRootPart") or gingerbread.PrimaryPart) then
                        local targetPart = gingerbread.PrimaryPart or gingerbread:FindFirstChild("HumanoidRootPart")
                        if targetPart then
                            humanoidRootPart.CFrame = targetPart.CFrame + Vector3.new(0, 5, 0)
                            wait(1.5)
                        end
                    end
                end
                
                wait(2)
            end
        end)
    else
        TweenService:Create(toggleFrame2, TweenInfo.new(0.3), {
            BackgroundColor3 = Color3.fromRGB(255, 80, 80)
        }):Play()
        toggleText2.Text = "OFF"
        
        print("Auto Farm DISABLED")
        
        if farmThread then
            task.cancel(farmThread)
            farmThread = nil
        end
    end
end

sectionFrame2.MouseButton1Click:Connect(function()
    toggleAutoFarm()
end)

toggleFrame2.MouseButton1Click:Connect(function()
    toggleAutoFarm()
end)

local function toggleAutoTeleport()
    autoTeleportEnabled = not autoTeleportEnabled
    
    if autoTeleportEnabled then
        TweenService:Create(toggleFrame3, TweenInfo.new(0.3), {
            BackgroundColor3 = Color3.fromRGB(80, 220, 100)
        }):Play()
        toggleText3.Text = "ON"
        
        print("Auto Teleport ENABLED")
        print("–ö–æ–æ—Ä–¥–∏–Ω–∞—Ç—ã: X=" .. COORDS_SPOT2.X .. ", Y=" .. COORDS_SPOT2.Y .. ", Z=" .. COORDS_SPOT2.Z)
        
        teleportThread = task.spawn(function()
            while autoTeleportEnabled do
                task.wait(30)
                
                local character = player.Character
                if not character then
                    character = player.CharacterAdded:Wait()
                end
                
                local humanoidRootPart = character:WaitForChild("HumanoidRootPart")
                
                local targetCFrame = CFrame.new(
                    COORDS_SPOT2.X,
                    COORDS_SPOT2.Y + 1,
                    COORDS_SPOT2.Z
                )
                humanoidRootPart.CFrame = targetCFrame
                
                print("Teleported to coordinates: X=" .. COORDS_SPOT2.X .. ", Y=" .. COORDS_SPOT2.Y .. ", Z=" .. COORDS_SPOT2.Z)
                
                wait(1)
            end
        end)
    else
        TweenService:Create(toggleFrame3, TweenInfo.new(0.3), {
            BackgroundColor3 = Color3.fromRGB(255, 80, 80)
        }):Play()
        toggleText3.Text = "OFF"
        
        print("Auto Teleport DISABLED")
        
        if teleportThread then
            task.cancel(teleportThread)
            teleportThread = nil
        end
    end
end

sectionFrame3.MouseButton1Click:Connect(function()
    toggleAutoTeleport()
end)

toggleFrame3.MouseButton1Click:Connect(function()
    toggleAutoTeleport()
end)

local dragging = false
local dragStart = Vector2.new(0, 0)
local startPos = UDim2.new(0, 0, 0, 0)

topBar.InputBegan:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseButton1 then
        dragging = true
        dragStart = input.Position
        startPos = mainContainer.Position
    end
end)

topBar.InputChanged:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseMovement then
        dragInput = input
    end
end)

UserInputService.InputChanged:Connect(function(input)
    if dragging and input.UserInputType == Enum.UserInputType.MouseMovement then
        local delta = input.Position - dragStart
        mainContainer.Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X, startPos.Y.Scale, startPos.Y.Offset + delta.Y)
    end
end)

UserInputService.InputEnded:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseButton1 then
        dragging = false
    end
end)

local isMinimized = false

local function toggleMinimize()
    isMinimized = not isMinimized
    
    if isMinimized then
        local tween = TweenService:Create(mainContainer, TweenInfo.new(0.4, Enum.EasingStyle.Quad), {
            Size = UDim2.new(0, 380, 0, 50)
        })
        tween:Play()
        
        contentFrame.Visible = false
        shadow.Visible = false
        
        TweenService:Create(closeBtn, TweenInfo.new(0.3), {
            Rotation = 180,
            BackgroundColor3 = Color3.fromRGB(100, 160, 220),
            Text = "‚ñ≤"
        }):Play()
        
        print("GUI minimized")
    else
        local tween = TweenService:Create(mainContainer, TweenInfo.new(0.4, Enum.EasingStyle.Quad), {
            Size = UDim2.new(0, 380, 0, 280)
        })
        tween:Play()
        
        wait(0.2)
        contentFrame.Visible = true
        shadow.Visible = true
        
        TweenService:Create(closeBtn, TweenInfo.new(0.3), {
            Rotation = 0,
            BackgroundColor3 = Color3.fromRGB(70, 130, 200),
            Text = "‚ùÑ"
        }):Play()
        
        print("GUI maximized")
    end
end

closeBtn.MouseButton1Click:Connect(function()
    toggleMinimize()
end)

closeBtn.MouseEnter:Connect(function()
    TweenService:Create(closeBtn, TweenInfo.new(0.2), {
        BackgroundColor3 = Color3.fromRGB(100, 170, 255),
        Rotation = isMinimized and 190 or 10
    }):Play()
end)

closeBtn.MouseLeave:Connect(function()
    TweenService:Create(closeBtn, TweenInfo.new(0.2), {
        BackgroundColor3 = isMinimized and Color3.fromRGB(100, 160, 220) or Color3.fromRGB(70, 130, 200),
        Rotation = isMinimized and 180 or 0
    }):Play()
end)

