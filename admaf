    if autoTeleportEnabled then
        TweenService:Create(toggleFrame3, TweenInfo.new(0.2), {
            BackgroundColor3 = Color3.fromRGB(80, 220, 100)
        }):Play()
    else
        TweenService:Create(toggleFrame3, TweenInfo.new(0.2), {
            BackgroundColor3 = Color3.fromRGB(255, 80, 80)
        }):Play()
    end
end)

print("Gingerbread Auto Farm UI loaded successfully!")
